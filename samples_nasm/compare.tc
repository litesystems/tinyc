int cmp1(int a, int b) {
    if (a + 1 > b) return 1;
    else return 0;
}

int cmp2(int a, int b) {
    if (a >= b + 1) return 1;
    else return 0;
}

int cmp3(int a, int b) {
    if (a > b + 1) return 1;
    else return 0;
}

int main() {
    test(cmp1(2, 3), 0);
    test(cmp2(4, 3), 1);
    test(cmp3(4, 2), 1);
}
